<edge>
  <id>string</id>
  <description>string</description>
  <datacenterMoid>string</datacenterMoid>
  <name>string</name>
  <fqdn>string</fqdn>
  <enableAesni>string</enableAesni>
  <enableFips>string</enableFips>
  <vseLogLevel>string</vseLogLevel>
  <vnics>
    <vnic>
      <index>string</index>
      <name>string</name>
      <type>string</type>
      <portgroupId>string</portgroupId>
      <addressGroups>
        <addressGroup>
          <primaryAddress>string</primaryAddress>
          <secondaryAddresses>
            <ipAddress>string</ipAddress>
          </secondaryAddresses>
          <subnetMask>string</subnetMask>
        </addressGroup>
      </addressGroups>
      <mtu>string</mtu>
      <enableProxyArp>string</enableProxyArp>
      <enableSendRedirects>string</enableSendRedirects>
      <isConnected>string</isConnected>
      <inShapingPolicy>
        <averageBandwidth>string</averageBandwidth>
        <peakBandwidth>string</peakBandwidth>
        <burstSize>string</burstSize>
        <enabled>string</enabled>
        <inherited>string</inherited>
      </inShapingPolicy>
      <outShapingPolicy>
        <averageBandwidth>string</averageBandwidth>
        <peakBandwidth>string</peakBandwidth>
        <burstSize>string</burstSize>
        <enabled>string</enabled>
        <inherited>string</inherited>
      </outShapingPolicy>
    </vnic>
  </vnics>
  <appliances>
    <applianceSize>string</applianceSize>
    <appliance>
      <haAdminState>string</haAdminState>
      <resourcePoolId>string</resourcePoolId>
      <datastoreId>string</datastoreId>
      <vmFolderId>string</vmFolderId>
    </appliance>
  </appliances>
  <cliSettings>
    <remoteAccess>string</remoteAccess>
    <userName>string</userName>
  </cliSettings>
  <features>
    <firewall>
      <defaultPolicy>
        <action>string</action>
        <loggingEnabled>string</loggingEnabled>
      </defaultPolicy>
      <firewallRules>
        <firewallRule>
          <id>string</id>
          <ruleTag>string</ruleTag>
          <name>string</name>
          <ruleType>string</ruleType>
          <source>
            <exclude>string</exclude>
            <!--Zero or more repetitions:-->
            <ipAddress>string</ipAddress>
            <!--Zero or more repetitions:-->
            <groupingObjectId>string</groupingObjectId>
            <!--Zero or more repetitions:-->
            <vnicGroupId>string</vnicGroupId>
          </source>
          <destination>
            <exclude>string</exclude>
            <!--Zero or more repetitions:-->
            <ipAddress>string</ipAddress>
            <!--Zero or more repetitions:-->
            <groupingObjectId>string</groupingObjectId>
            <!--Zero or more repetitions:-->
            <vnicGroupId>string</vnicGroupId>
          </destination>
          <application>
            <applicationId>string</applicationId>
            <service>
              <protocol>string</protocol>
              <port>string</port>
              <sourcePort>string</sourcePort>
            </service>
          </application>
          <action>string</action>
          <enabled>string</enabled>
          <loggingEnabled>string</loggingEnabled>
          <matchTranslated>string</matchTranslated>
        </firewallRule>
      </firewallRules>
    </firewall>
    <routing>
      <staticRouting>
        <defaultRoute>
          <vnic>string</vnic>
          <gatewayAddress>string</gatewayAddress>
          <description>string</description>
        </defaultRoute>
        <staticRoutes>
          <route>
            <vnic>string</vnic>
            <network>string</network>
            <nextHop>string</nextHop>
            <type>string</type>
          </route>
        </staticRoutes>
      </staticRouting>
      <ospf>
        <enabled>string</enabled>
      </ospf>
    </routing>
    <highAvailability>
      <enabled>string</enabled>
      <declareDeadTime>string</declareDeadTime>
      <logging>
        <enable>string</enable>
        <logLevel>string</logLevel>
      </logging>
    </highAvailability>
    <syslog>
      <protocol>string</protocol>
      <serverAddresses>
        <ipAddress>string</ipAddress>
      </serverAddresses>
    </syslog>
    <ipsec>
      <enabled>string</enabled>
      <logging>
        <enable>string</enable>
        <logLevel>string</logLevel>
      </logging>
      <sites>
        <site>
          <enabled>string</enabled>
          <name>string</name>
          <localId>string</localId>
          <localIp>string</localIp>
          <peerId>string</peerId>
          <encryptionAlgorithm>string</encryptionAlgorithm>
          <mtu>string</mtu>
          <enablePfs>string</enablePfs>
          <dhGroup>string</dhGroup>
          <localSubnets>
            <subnet>string</subnet>
          </localSubnets>
          <peerSubnets>
            <subnet>string</subnet>
          </peerSubnets>
          <psk>string</psk>
          <authenticationMode>string</authenticationMode>
        </site>
      </sites>
      <global>
        <caCertificates>string</caCertificates>
        <crlCertificates>string</crlCertificates>
      </global>
    </ipsec>
    <dhcp>
      <enabled>string</enabled>
      <staticBindings>
        <staticBinding>
          <autoConfigureDNS>string</autoConfigureDNS>
          <bindingId>string</bindingId>
          <vmId>string</vmId>
          <vnicId>string</vnicId>
          <hostname>string</hostname>
          <ipAddress>string</ipAddress>
          <defaultGateway>string</defaultGateway>
          <leaseTime>string</leaseTime>
        </staticBinding>
      </staticBindings>
      <ipPools>
        <ipPool>
          <autoConfigureDNS>string</autoConfigureDNS>
          <poolId>string</poolId>
          <ipRange>string</ipRange>
          <defaultGateway>string</defaultGateway>
          <leaseTime>string</leaseTime>
        </ipPool>
      </ipPools>
      <logging>
        <enable>string</enable>
        <logLevel>string</logLevel>
      </logging>
    </dhcp>
    <nat>
      <natRules>
        <natRule>
          <ruleId>string</ruleId>
          <ruleTag>string</ruleTag>
          <ruleType>string</ruleType>
          <action>string</action>
          <vnic>string</vnic>
          <originalAddress>string</originalAddress>
          <translatedAddress>string</translatedAddress>
          <dnatMatchSourceAddress>string</dnatMatchSourceAddress>
          <snatMatchDestinationAddress>string</snatMatchDestinationAddress>
          <loggingEnabled>string</loggingEnabled>
          <enabled>string</enabled>
          <protocol>string</protocol>
          <originalPort>string</originalPort>
          <translatedPort>string</translatedPort>
          <dnatMatchSourcePort>string</dnatMatchSourcePort>
          <snatMatchDestinationPort>string</snatMatchDestinationPort>
        </natRule>
      </natRules>
    </nat>
  </features>
  <autoConfiguration>
    <enabled>string</enabled>
    <rulePriority>string</rulePriority>
  </autoConfiguration>
</edge>